{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@/": ["./src/"],

      "ui/*": ["./src/_core/ui/*"],
      "ui": ["./src/_core/ui"],
      "shadcn/*": ["./src/_core/ui/shadcn/*"],
      "shadcn": ["./src/_core/ui/shadcn"],
      "aceternity/*": ["./src/_core/ui/aceternity/*"],
      "aceternity": ["./src/_core/ui/aceternity"],
      "theme/*": ["./src/_core/ui/theme/*"],
      "theme": ["./src/_core/ui/theme"],

      "logic/*": ["./src/_core/logic/*"],
      "logic": ["./src/_core/logic"],
      "config/*": ["./src/_core/logic/config/*"],
      "logic": ["./src/_core/logic/config"],
      "hooks/*": ["./src/_core/logic/hooks/*"],
      "hooks": ["./src/_core/logic/hooks"],
      "utils/*": ["./src/_core/logic/utils/*"],
      "utils": ["./src/_core/logic/utils"],

      "@modules/*": ["./src/app/(modules)/*"],
      "@login/*": ["./src/app/login/_internal/*"],
      "@prenota/*": ["./src/app/(modules)/prenota/_internal/*"],
      "@pedido/*": ["./src/app/(modules)/prenota/pedido/_internal/*"],
      "@inclusao/*": ["./src/app/(modules)/prenota/inclusao/_internal/*"],
      "@controle/*": ["./src/app/(modules)/controle/_internal/*"],
      "@borracharia/*": ["./src/app/(modules)/controle/borracharia/_internal/*"],
      "@portaria/*": ["./src/app/(modules)/controle/portaria/_internal/*"],
      "@documentacao/*": ["./src/app/documentacao/_internal/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/(modules)/prenota/inclusao/_internal/(api)/getXmlDetails.ts"
  ],
  "exclude": ["node_modules"]
}
